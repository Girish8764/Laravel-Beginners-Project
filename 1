server {
    listen 80;
    server_name 10.1.4.223;

    root /php/Laravel-12-Beginners-Project/public;
    index index.php index.html;

    location / {
        try_files $uri $uri/ /index.php?$query_string;
    }

    location ~ \.php$ {
        include snippets/fastcgi-php.conf;
        fastcgi_pass 127.0.0.1:9025;
	fastcgi_param SCRIPT_FILENAME /php/Laravel-12-Beginners-Project/public$fastcgi_script_name;
    }

    location ~ /\.ht {
        deny all;
    }
}

